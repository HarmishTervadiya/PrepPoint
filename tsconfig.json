{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/api": ["./api"],
      "@/components": ["./components"],
      "@/utils": ["./utils"],
      "@/redux-toolkit": ["./redux-toolkit"],
      "@/types": ["./types"],
      "@/assets": ["./assets"],
      "@/themes": ["./themes"],
      "@/constants": ["./constants"],
      "@/hooks": ["./hooks"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
